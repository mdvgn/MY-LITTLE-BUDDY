<div class="show-container">

<h1><%= @dwarf.nickname %></h1>
<h2>Size : <%= @dwarf.size %> cm</h2>
<h3>Gender : <%= @dwarf.gender %></h3>
<h3>Description : <%= @dwarf.description %></h3>
<h3>Location : <%= @dwarf.location %></h3>
<% @dwarf.photos.each do |photo| %>
<%= cl_image_tag photo.key %>
<% end %>
<%  @dwarf.dwarf_skills.each do |skill| %>
<h3><%=  Skill.find(skill.skill_id).skill %></h3>
<% end %>


</div>
