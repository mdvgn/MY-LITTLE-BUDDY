<form action="" method="get">
  <div class="product-search">
      <div class="search-element">
          <label class="search-label">Activity ?</label>
          <input class="search-input" type="text" autocomplete="on" placeholder="Ex: Midget tossing" name="query">
      </div>
      <div class="search-element">
          <label class="search-label">Location ?</label>
          <input class="search-input" type="text" placeholder="City, State" class="" autocomplete="on" name="location">
      </div>
      <a type="submit" class="search-button">Search</a>
  </div>
</form>
